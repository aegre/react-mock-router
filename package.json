  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublish": "rimraf lib && babel src --out-dir lib --ignore __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkoeze/react-mock-router.git"
  },
  "author": "Robert Koeze <rkoeze@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rkoeze/react-mock-router/issues"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "20.0.4",
    "prop-types": "15.5.10",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.4.2",
    "regenerator-runtime": "0.10.5",
    "rimraf": "2.4.3"
